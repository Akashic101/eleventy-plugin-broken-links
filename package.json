{
  "name": "eleventy-plugin-broken-links",
  "version": "1.6.2",
  "description": "An Eleventy plugin to check for broken external links",
  "homepage": "https://github.com/bradleyburgess/eleventy-plugin-broken-links",
  "main": ".eleventy.js",
  "scripts": {
    "start": "eleventy --serve",
    "build": "eleventy",
    "lint": "eslint ./",
    "lint:fix": "eslint ./ --fix",
    "bump": "npm --no-git-tag-version version",
    "coverage": "coverage",
    "test": "nyc --reporter=lcov ava",
    "test:watch": "ava --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bradleyburgess/eleventy-plugin-broken-links.git"
  },
  "keywords": [
    "11ty",
    "11ty-plugin",
    "eleventy",
    "eleventy-plugin",
    "broken-links"
  ],
  "author": {
    "name": "Bradley Burgess",
    "url": "https://github.com/bradleyburgess"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bradleyburgess/eleventy-plugin-broken-links/issues"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "ava": "^4.2.0",
    "codecov": "^3.8.3",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "ninos": "^3.0.0",
    "nyc": "^15.1.0",
    "prettier": "^2.6.2"
  },
  "dependencies": {
    "@11ty/eleventy-fetch": "^3.0.0",
    "kleur": "^4.1.4",
    "minimatch": "^5.0.1",
    "node-html-parser": "^5.3.3",
    "url-status-code": "^2.0.0"
  },
  "ava": {
    "files": [
      "__tests__/**/*.js",
      "**/*.test.js"
    ]
  }
}
